        <complexContent>
            <extension base="person:KontaktKanalBaseType">
                <choice>
                    <element ref="oio8:EmailAddressIdentifier" oio:content="emailadresse"/>
                    <element ref="person:Telefon" oio:content="telefon"/>
                    <element ref="person:AndenKontaktKanal" oio:content="andenkontaktkanal"/>
                </choice>
            </extension>
        </complexContent>
    </complexType>

