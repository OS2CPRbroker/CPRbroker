    <!-- <element name="LivStatusKode" type="person:LivStatusKodeType" oio:definition="urn:oio:sagdok:livstatuskode:BDD:1.0.0"/> -->
    
    <!-- <element name="CivilStatusKode" type="person:CivilStatusKodeType" oio:definition="urn:oio:sagdok:civilstatuskode:BDD:1.0.0"/> -->
    
    <element name="PersonRelation" type="person:PersonRelationType" oio:definition="urn:oio:sagdok:personrelation:ADD:1.0.0"/>
