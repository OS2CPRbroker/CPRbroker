    <element name="UkendtBorger" type="person:UkendtBorgerType" oio:definition="urn:oio:sagdok:ukendtborger:ADD:1.0.0"/>
    <complexType name="UkendtBorgerType" oio:definition="urn:oio:sagdok:ukendtborger:ADD:1.0.0">
        <sequence>
            <element ref="oio10:PersonCivilRegistrationReplacementIdentifier" oio:content="erstatningscpr"/>
        </sequence>
