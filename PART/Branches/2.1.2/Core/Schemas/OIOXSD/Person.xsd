    <complexType name="AdresseType" oio:definition="urn:oio:sagdok:adresse:ADD:1.0.0">
        <choice>
            <element ref="person:DanskAdresse" oio:content="danskadresse"/>
            <element ref="person:GroenlandAdresse" oio:content="groenlandskadresse"/>
            <element ref="person:VerdenAdresse" oio:content="verdensadresse"/>
        </choice>
    </complexType>

