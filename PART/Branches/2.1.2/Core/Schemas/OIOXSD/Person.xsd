            <element ref="oio9:PersonNationalityCode" oio:content="statsborgerskab"/>
            <element ref="person:NavneAdresseBeskyttelseIndikator" oio:content="navneadressebeskyttelse"/>
            <element ref="person:TelefonNummerBeskyttelseIndikator" oio:content="telefonnummerbeskyttelse"/>
