                <sequence>
                    <element ref="person:Registrering" maxOccurs="unbounded" oio:content="registreringer"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="AttributListe" type="person:AttributListeType" oio:definition="urn:oio:sagdok:personattributliste:ADD:1.0.0"/>
